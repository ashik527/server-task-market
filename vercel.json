{
  "version": 2,
  "builds": [{ "src": "api/**/*.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/api/tasks/bid", "dest": "/api/tasks/bid.js" },
    { "src": "/api/tasks/([^/]+)", "dest": "/api/tasks/[id].js" },
    { "src": "/api/tasks", "dest": "/api/tasks/index.js" },
    { "src": "/api/seed", "dest": "/api/seed.js" }
  ]
}